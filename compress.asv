function [compressedFile, ratio] = compress(path, type, domain, loss)
signal=0;
switch path
    case '*.mat'
        load(path);
        if(type=='ECG')
            signal = val;
        else(type=='Ultrasound')
            signal = ult_sig;
    case '*.wav'
        [signal, fs] = audioread(path);
    case '*.xlsx'
        signal = xlsread(path);
end

